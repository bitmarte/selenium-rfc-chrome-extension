{
    "name": "__MSG_extensionName__",
    "description": "__MSG_extensionDescription__",
    "default_locale": "en",
    "version": "1.0.0",
    "background": {
        "scripts": [
            "js/background-min.js"
        ]
    },
    "content_scripts": [{
        "persistent": false,
        "matches": ["<all_urls>"],
        "js": ["js/content-min.js"],
        "all_frames": true
    }],
    "browser_action": {
        "default_icon": "images/icon.png"
    },
    "icons": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "64": "images/icon.png",
        "128": "images/icon.png"
    },
    "commands": {},
    "permissions": ["activeTab", "gcm", "notifications", "contextMenus", "history"],
    "manifest_version": 2
}